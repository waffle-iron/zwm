{
    "autoReload" : true,
    "mod": "alt",
    "key_bindings": {
        "terminal" : {
            "key": "return",
            "type": "application",
            "action": "/Applications/iTerm.app" 
        },
        "window_left" : {
            "key" : "h",
            "type" : "window",
            "action": "window_left"
        },
        "window_right" : {
            "key" : "l",
            "type" : "window",
            "action": "window_right"
        },
        "window_up" : {
            "key" : "k",
            "type" : "window",
            "action": "window_up"
        },
        "window_down" : {
            "key" : "j",
            "type" : "window",
            "action": "window_down"
        },
        "window_next" : {
            "key" : "]",
            "type" : "window",
            "action": "window_next"
        },
        "window_previous" : {
            "key" : "[",
            "type" : "window",
            "action": "window_previous"
        },
        "window_tiling_mode" : {
            "key" : "t",
            "type": "window",
            "action": "window_tiling_toggle"
        }
    },
    "window_management" : {
        "mode" : "monocle",
        "gap" : "20",
        "move_amount" : 10
    }
}